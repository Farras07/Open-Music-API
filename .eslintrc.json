{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "import/no-extraneous-dependencies": "disable"
    }
}
